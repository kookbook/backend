ien,Creme,Dessert,Europa,Festlich,Frühling,Italien,Sommer,Süßspeise,42631014553456,simpel;kw=rezepte&uid=' + window.CK.ems.handleEmsUId() // Targeting-Werte als HTML Parameter (String),\n            );\n        }\n        </script>\n    </div>\n    <div class=\"content-wrapper clearfix\">\n\n        <div class=\"alert alert-info js-random-recipe__cta-container is-hidden\"><button class=\"close\" data-dismiss=\"alert\">x</button>Ein <a href=\"/rezepte/zufallsrezept/?t=1537880251\"><strong>weiteres Zufallsrezept</strong></a></div>\n\n        \n        <div class=\"js-search-result-sibling-navigator search-result-sibling-navigator clearfix\">\n    <a class=\"js-search-result-sibling-navigator__previous search-result-sibling-navigator__button f-l\">\n        <svg class=\"search-result-sibling-navigator__icon m-r\" viewBox=\"0 0 8 8\">\n            <path d=\"M4 0l-4 4 4 4v-8z\" transform=\"translate(2)\" fill=\"currentColor\"></path>\n        </svg>voriges Rezept</a>\n    <a class=\"js-search-result-sibling-navigator__back-to-search search-result-sibling-navigator__back-to-search\" >zur Ergebnisliste</a>\n    <a class=\"js-search-result-sibling-navigator__next search-result-sibling-navigator__button f-r\">nächstes Rezept<svg class=\"search-result-sibling-navigator__icon m-l\" viewBox=\"0 0 8 8\">\n            <path d=\"M0 0v8l4-4-4-4z\" transform=\"translate(2)\" fill=\"currentColor\"></path>\n        </svg>\n    </a>\n</div>\n<script type=\"text/javascript\">\n    (function searchResultSiblingNavigator() {\n\n        \n        var options = {\n            selectors: {\n                container: \".js-search-result-sibling-navigator\",\n                prevLink: \".js-search-result-sibling-navigator__previous\",\n                nextLink: \".js-search-result-sibling-navigator__next\",\n                backToSearch: \".js-search-result-sibling-navigator__back-to-search\"\n            }\n        };\n\n        var container = document.querySelector(options.selectors.container);\n        var prevLink = document.querySelector(options.selectors.prevLink);\n        var nextLink = document.querySelector(options.selectors.nextLink);\n        var backToSearch = document.querySelector(options.selectors.backToSearch);\n\n        var recipeBrowsingData;\n\n        // Feature detect + local reference\n        var storage;\n        var fail;\n        var uid;\n        try {\n            uid = new Date;\n            (storage = window.localStorage).setItem(uid, uid);\n            fail = storage.getItem(uid) != uid;\n            storage.removeItem(uid);\n            fail && (storage = false);\n        } catch (exception) {}\n\n        if (storage) {\n            if (storage.recipeBrowsingData) {\n                recipeBrowsingData = JSON.parse(storage.recipeBrowsingData);\n            }\n            if (!recipeBrowsingData || recipeBrowsingData.results.length === 1) {\n                return;\n            }\n\n            var index = 0;\n            var url = document.location.toString();\n\n            recipeBrowsingData.results.forEach(function(result) {\n                if (url.indexOf(result.url) >= 0) {\n                    var previousResult = recipeBrowsingData.results[index - 1];\n                    if (previousResult) {\n                        // add prev url\n                        prevLink.setAttribute(\"href\", previousResult.url);\n\n                        // show prev item\n                        if (prevLink.classList)\n                            prevLink.classList.add('is-enabled');\n                        else\n                            prevLink.className += ' is-enabled';\n                    }\n\n                    var nextResult = recipeBrowsingData.results[index + 1];\n                    if (nextResult) {\n                        // add next url\n                        nextLink.setAttribute('href', nextResult.url);\n\n                        // show next item\n                        if (nextLink.classList)\n                            nextLink.classList.add('is-enabled');\n                        else\n                            nextLink.className += ' is-enabled';\n                    }\n\n                    // add searchUrl to back link\n                    var searchUrl = recipeBrowsingData.searchHost\n                        + recipeBrowsingData.searchPath\n                        + (recipeBrowsingData.searchHash ? recipeBrowsingData.searchHash : '');\n\n                    backToSearch.setAttribute('href', searchUrl);\n\n                    // show search result sibling navigation container\n                    container.style.display = 'block';\n                }\n\n                index++;\n            });\n        }\n    })();\n</script>\n\n        <div class=\"content\">\n\n                        <div id=\"text-ads-oben\" class=\"\">\n                            </div>\n\n            \n            <div id=\"recipe-image-buttons\" class=\"clearfix\">\n\n                <div id=\"slideshow\">\n\n            <div class=\"slider-wrapper\">\n\n            <button type=\"button\" class=\"js-image-slider__button--prev image-slider__button image-slider__button--prev\">\n                <div class=\"button__icon--image-slider\">\n                    <span class=\"is-visually-hidden\">vorheriges Bild</span>\n                    <svg class=\"icon__arrow--image-slider\" width=\"16\" height=\"27\" viewBox=\"0 0 16 27\" xmlns=\"http://www.w3.org/2000/svg\">\n                        <path d=\"M15.7 2.24c0 .233-.09.44-.27.62l-10.6 10.6 10.6 10.603c.18.18.27.387.27.62 0 .235-.09.44-.27.62l-1.348 1.35c-.18.18-.387.27-.62.27-.234 0-.44-.09-.62-.27L.27 14.083c-.18-.18-.27-.388-.27-.622 0-.232.09-.44.27-.62L12.84.27c.18-.18.388-.27.62-.27.235 0 .442.09.622.27l1.35 1.35c.18.178.268.385.268.62z\" />\n                    </svg>\n                </div>\n            </button>\n\n            <div id=\"slider\" class=\"\">\n                                    <figure class=\"recipe-image\">\