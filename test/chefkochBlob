       var c = ca[i].trim();\n                    if (c.indexOf(name) === 0) return c.substring(name.length, c.length);\n                }\n                return \"\";\n            };\n\n            /**\n            * EMS User Id handler\n            *\n            * This function returns a EMS User Id. It checks for an existing \"ems_uid\" cookie and retrieves its value or creates a new User Id\n            * through CryptoJS.MD5 and stores it in the above-mentioned cookie.\n            *\n            * @requires CryptoJS (contained in asynchronously loaded /js/mobile/md5.js)\n            * @returns {String} EMS User Id\n            */\n            window.CK.ems.handleEmsUId = function() {\n                var ems_uid = window.CK.ems.getCookie(\"ems_uid\");\n                var ems_new_uid;\n\n                if (ems_uid !== \"\") {\n                    return ems_uid;\n                } else {\n                    ems_new_uid = CryptoJS.MD5(navigator.userAgent + \"-\" + (new Date()).getTime());\n                    window.CK.ems.setCookie(\"ems_uid\",ems_new_uid,30);\n                    return ems_new_uid;\n                }\n            };\n\n        })();\n\n        // jQuery.getScript verwendet cache-busting, daher diese Hilfsfunktion\n        // s. https://api.jquery.com/jquery.getscript/#caching-requests\n        jQuery.cachedScript = function( url, options ) {\n            options = $.extend( options || {}, {\n                dataType: \"script\",\n                cache: true,\n                url: url\n            });\n\n            return jQuery.ajax( options );\n        };\n\n        // Die mobile Werbung soll abgerufen werden:\n        // - auf nicht-responsiven Seiten, die $werbung[mobile][footer] nutzen (Mobile Seite!)\n        // - auf responsiven Seiten, wenn die Seite in der mobilen Ansicht ist\n        // Auf der normalen, nicht-responsiven Seite wird dieser Code garnicht eingebunden (kein $werbung[mobile])\n        if (!CK.core.isLocationRespons